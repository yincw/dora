{
  "name": "@yincw/dora",
  "version": "1.0.0",
  "description": "A library of JavaScript tools",
  "private": true,
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "sideEffects": false,
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "build:es": "tsc --target ES2015 --module ES2015 --rootDir src --outDir es",
    "build:lib": "tsc --target ES5 --module CommonJS --rootDir src --outDir lib",
    "release": "rollup -c && rollup -c rollup.config.min.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "e2e": "cypress open",
    "clean": "rimraf lib/* && rimraf es/* && rimraf dist/*",
    "clean:test": "rimraf src/* && rimraf __tests__/*",
    "gen": "node ./scripts/generator"
  },
  "keywords": [
    "TypeScript",
    "Jest",
    "tools",
    "utils"
  ],
  "author": "yincw",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.10",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.13",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.6",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7",
    "uglify-es": "^3.3.9"
  }
}
