{
  "name": "@yincw/dora",
  "version": "0.0.10",
  "description": "A library of JavaScript utils",
  "main": "lib/index.js",
  "browser": "dist/dora.min.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "lib",
    "es",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -wc",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress open",
    "prepublish": "npm run build && npm run docs && git add . && cz && standard-version",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run build:amd && npm run build:dist",
    "build:dist": "cross-env NODE_ENV=production rollup -c",
    "build:amd": "tsc --outDir dist --module amd",
    "build:lib": "tsc --outDir lib --module commonjs --declaration true",
    "build:es": "tsc --outDir es --module es2015 --declaration true",
    "clean": "rimraf dist/* && rimraf lib/* && rimraf es/* && rimraf coverage/* && rimraf docs/* && rimraf temp/*",
    "docs": "api-extractor run && api-documenter markdown --input-folder temp --output-folder docs",
    "gen": "node scripts/generator.js --mode=gen",
    "del": "node scripts/generator.js --mode=del"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & jest",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*./src/**/*.{ts,js,less,css,json,md}": [
      "prettier --write --ignore-unknown",
      "eslint 'src/**/*.{ts,js}'"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@microsoft/api-documenter": "^7.8.48",
    "@microsoft/api-extractor": "^7.9.13",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.13",
    "prettier": "2.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "core-js": "^3.37.1"
  },
  "keywords": [
    "Dora",
    "Utils",
    "Tools",
    "TypeScript",
    "Babel",
    "Rollup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yincw/dora.git"
  },
  "author": "yincw",
  "license": "MIT"
}
